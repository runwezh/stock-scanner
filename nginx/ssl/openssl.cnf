# openssl.cnf 简易内容
[ req ]
default_bits       = 2048
default_md         = sha256
distinguished_name = req_distinguished_name
x509_extensions    = v3_req
prompt             = no

[ req_distinguished_name ]
C  = CN
ST = localhost
L  = localhost
O  = localhost
OU = localhost
CN = localhost

[ v3_req ]
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost